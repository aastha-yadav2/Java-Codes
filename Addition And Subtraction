import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    System.out.println("Enter two Number");
	    int x = sc.nextInt();
	    int y = sc.nextInt();
	    System.out.println("Sum is :"+(x+y));
	    System.out.println("Subtraction is :"+(x-y));
      System.out.println("Subtraction is :"+(x*y));
		 System.out.println("Subtraction is :"+(x/y));
	}
}
